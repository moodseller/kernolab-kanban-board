{
  "overrides": [
    {
      "files": ["*.js", "*.jsx", "*.ts", "*.tsx", "*.scss"],
      "options": {
        "arrowParens": "always",
        "singleQuote": true,
        "jsxSingleQuote": true,
        "tabWidth": 4,
        "printWidth": 120,
        "trailingComma": "all"
      }
    }
  ]
}
